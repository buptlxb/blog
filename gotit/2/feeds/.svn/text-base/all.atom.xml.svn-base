    &lt;span class="na"&gt;.word&lt;/span&gt;   &lt;span class="mi"&gt;0&lt;/span&gt;&lt;span class="p"&gt;,&lt;/span&gt;&lt;span class="mi"&gt;0&lt;/span&gt;         &lt;span class="c"&gt;# idt base=0L&lt;/span&gt;


    &lt;span class="nf"&gt;cld&lt;/span&gt;&lt;span class="err"&gt;;&lt;/span&gt;&lt;span class="no"&gt;rep&lt;/span&gt;&lt;span class="err"&gt;;&lt;/span&gt;&lt;span class="no"&gt;stosl&lt;/span&gt;
